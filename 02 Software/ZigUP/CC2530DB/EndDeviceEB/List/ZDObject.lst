   2516          /*********************************************************************
   2517           * @fn      ZDMatchSendState()
   2518           *
   2519           * @brief   State machine for the End device match message algorithm.
   2520           *
   2521           * @param  reason - state of algoritm
   2522           * @param  status - initial message status
   2523           * @param  TransSeq - next transaction sequence number
   2524           *
   2525           * @return  FALSE if error and we are not currently matching, TRUE
   2526           *          if success.
   2527           */
   2528          uint8 ZDMatchSendState( uint8 reason, uint8 status, uint8 TransSeq )
   2529          {
   2530            uint8 *dstIEEEAddr = NULL;
   2531            uint8 dstEP = 0xFF;
   2532            zAddrType_t dstAddr;
   2533            zAddrType_t destinationAddr;
   2534            uint16 msgType;
   2535            uint16 clusterID = 0xFFFF;
   2536            ZDEndDeviceBind_t *ed = NULL;
   2537            uint8 rspStatus = ZDP_SUCCESS;
   2538          
   2539            if ( matchED == NULL )
   2540            {
   2541              return ( FALSE );
   2542            }
   2543          
   2544            // Check sequence number
   2545            if ( reason == ZDMATCH_REASON_BIND_RSP || reason == ZDMATCH_REASON_UNBIND_RSP )
   2546            {
   2547              if ( TransSeq != matchED->transSeq )
   2548              {
   2549                return( FALSE ); // ignore the message
   2550              }
   2551            }
   2552          
   2553            // turn off timer
   2554            APS_SetEndDeviceBindTimeout( 0, ZDO_EndDeviceBindMatchTimeoutCB );
   2555          
   2556            if ( reason == ZDMATCH_REASON_TIMEOUT )
   2557            {
   2558              rspStatus = ZDP_TIMEOUT;    // The process will stop
   2559            }
   2560          
   2561            if ( reason == ZDMATCH_REASON_START || reason == ZDMATCH_REASON_BIND_RSP )
   2562            {
   2563              matchED->sending = ZDMATCH_SENDING_UNBIND;
   2564          
   2565              if ( reason == ZDMATCH_REASON_BIND_RSP && status != ZDP_SUCCESS )
   2566              {
   2567                rspStatus = status;
   2568              }
   2569            }
   2570            else if ( reason == ZDMATCH_REASON_UNBIND_RSP )
   2571            {
   2572              if ( status == ZDP_SUCCESS )
   2573              {
   2574                matchED->sending = ZDMATCH_SENDING_UNBIND;
   2575              }
   2576              else
   2577              {
   2578                matchED->sending = ZDMATCH_SENDING_BIND;
   2579              }
   2580            }
   2581          
   2582            if ( reason != ZDMATCH_REASON_START && matchED->sending == ZDMATCH_SENDING_UNBIND )
   2583            {
   2584              // Move to the next cluster ID
   2585              if ( matchED->ed1numMatched )
   2586              {
   2587                matchED->ed1numMatched--;
   2588              }
   2589              else if ( matchED->ed2numMatched )
   2590              {
   2591                matchED->ed2numMatched--;
   2592              }
   2593            }
   2594          
   2595            // What message do we send now
   2596            if ( matchED->ed1numMatched )
   2597            {
   2598              ed = &(matchED->ed1);
   2599              clusterID = matchED->ed1Matched[matchED->ed1numMatched-1];
   2600              dstIEEEAddr = matchED->ed2.ieeeAddr;
   2601              dstEP = matchED->ed2.endpoint;
   2602            }
   2603            else if ( matchED->ed2numMatched )
   2604            {
   2605              ed = &(matchED->ed2);
   2606              clusterID = matchED->ed2Matched[matchED->ed2numMatched-1];
   2607              dstIEEEAddr = matchED->ed1.ieeeAddr;
   2608              dstEP = matchED->ed1.endpoint;
   2609            }
   2610          
   2611            dstAddr.addrMode = Addr16Bit;
   2612          
   2613            // Send the next message
   2614            if ( (rspStatus == ZDP_SUCCESS) && ed )
   2615            {
   2616              // Send unbind/bind message to source
   2617              if ( matchED->sending == ZDMATCH_SENDING_UNBIND )
   2618              {
   2619                msgType = Unbind_req;
   2620              }
   2621              else
   2622              {
   2623                msgType = Bind_req;
   2624              }
   2625          
   2626              dstAddr.addr.shortAddr = ed->srcAddr;
   2627          
   2628              // Save off the transaction sequence number
   2629              matchED->transSeq = ZDP_TransID;
   2630          
   2631              destinationAddr.addrMode = Addr64Bit;
   2632              osal_cpyExtAddr( destinationAddr.addr.extAddr, dstIEEEAddr );
   2633          
   2634              ZDP_BindUnbindReq( msgType, &dstAddr, ed->ieeeAddr, ed->endpoint, clusterID,
   2635                  &destinationAddr, dstEP, ed->SecurityUse );
   2636          
   2637              // Set timeout for response
   2638              APS_SetEndDeviceBindTimeout( AIB_MaxBindingTime, ZDO_EndDeviceBindMatchTimeoutCB );
   2639            }
   2640            else
   2641            {
   2642              // Send the response messages to requesting devices
   2643              // send response to first requester
   2644              dstAddr.addr.shortAddr = matchED->ed1.srcAddr;
   2645              ZDP_EndDeviceBindRsp( matchED->ed1.TransSeq, &dstAddr, rspStatus, matchED->ed1.SecurityUse );
   2646          
   2647              // send response to second requester
   2648              if ( matchED->state == ZDMATCH_SENDING_BINDS )
   2649              {
   2650                dstAddr.addr.shortAddr = matchED->ed2.srcAddr;
   2651                ZDP_EndDeviceBindRsp( matchED->ed2.TransSeq, &dstAddr, rspStatus, matchED->ed2.SecurityUse );
   2652              }
   2653          
   2654              // Process ended - release memory used
   2655              ZDO_RemoveMatchMemory();
   2656            }
   2657          
   2658            return ( TRUE );
   2659          }
